<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="formInput">
        <attr name="label" format="string" />
        <attr name="required" format="boolean" />
        <attr name="type" format="enum">
            <enum name="text" value="1" />
            <enum name="phone" value="2" />
            <enum name="date" value="3" />
            <enum name="dict" value="4" />
            <enum name="number" value="5" />
            <enum name="action" value="6" />
        </attr>
        <attr name="dictName" format="string" />
        <attr name="inputHint" format="string" />
    </declare-styleable>
</resources>